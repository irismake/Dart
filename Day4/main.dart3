void main() {
  List<Map<String, String>> people = [
    {
      'name' : '로제',
      'group' : '블랙핑크',
    },
    {
      'name' : '지수',
      'group' : '블랙핑크',
    },
    {
      'name' : 'RM',
      'group' : 'bts',
    },
    {
      'name' : '뷔',
      'group' : 'bts',
    }
  ];
  
  print(people);
  
  final blackPink = people.where((x) => x['group'] == '블랙핑크').toList();
  
  print(blackPink);
  
  final bts = people.where((x) => x['group'] == 'bts').toList();
  
  print(bts);
}
